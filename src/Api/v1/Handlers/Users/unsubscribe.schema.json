{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Unsubscribe",
  "type": "object",
  "$id": "POST /api/v1/users/un-subscribe",
  "title": "Unsubscribe",
  "examples": [{
    "user_id": 1,
    "email": "test@test.com",
    "list_id": 1,
    "variant_id": 1,
    "send_accompanying_emails": true
  },
  {
    "user_id": 1,
    "email": "test@test.com",
    "list_code": "123",
    "variant_id": null,
    "send_accompanying_emails": true
  }],
  "required": [
    "email",
    "user_id"
  ],
  "anyOf": [
    {"required": ["list_id"]},
    {"required": ["list_code"]}
  ],
  "additionalProperties": false,
  "properties": {
    "user_id": {
      "type": "integer"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "list_id": {
      "type": "integer"
    },
    "list_code": {
      "type": "string"
    },
    "variant_id": {
      "anyOf": [
        {"type": "integer"},
        {"type": "null"}
      ]
    },
    "variant_code": {
      "anyOf": [
        {"type": "string"},
        {"type": "null"}
      ]
    },
    "send_accompanying_emails": {
      "type": "boolean"
    },
    "utm_params": {
      "title": "Utm params",
      "type": "object",
      "properties": {
        "utm_source": {
          "type": "string"
        },
        "utm_medium": {
          "type": "string"
        },
        "utm_campaign": {
          "type": "string"
        },
        "utm_content": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "rtm_params": {
      "title": "Rtm params",
      "type": "object",
      "properties": {
        "rtm_source": {
          "type": "string"
        },
        "rtm_medium": {
          "type": "string"
        },
        "rtm_campaign": {
          "type": "string"
        },
        "rtm_content": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}